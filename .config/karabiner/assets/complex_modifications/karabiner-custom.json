{
  "title": "Left ctrl + d / u to PgUp / PgDwn",
  "rules": [
    {
      "description": "Left ctrl + d / u to PgUp / PgDwn",
      "manipulators": [
        {
          "from": {
            "key_code": "d",
            "modifiers": {
              "mandatory": ["left_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "page_down"
            }
          ],
          "type": "basic"
        },
        {
          "from": {
            "key_code": "u",
            "modifiers": {
              "mandatory": ["left_control"],
              "optional": ["any"]
            }
          },
          "to": [
            {
              "key_code": "page_up"
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
